{
  "name": "old-growth-frontend",
  "version": "0.0.1",
  "description": "Frontend for old growth project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcgov/nr-old-growth-integration.git"
  },
  "keywords": [
    "openshift",
    "pipeline",
    "node",
    "typescript",
    "js",
    "devops"
  ],
  "author": "Derek Roberts, Catherine Meng, Maria Martinez",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/bcgov/nr-old-growth-integration/issues"
  },
  "homepage": "https://github.com/bcgov/nr-old-growth-integration",
  "private": true,
  "scripts": {
    "start": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test": "vitest --environment jsdom --coverage",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "axios": "^1.0.0",
    "bootstrap": "^5.2.2",
    "bootstrap-icons-vue": "^1.8.1",
    "bootstrap-vue": "^2.22.0",
    "bootstrap-vue-3": "^0.3.11",
    "core-js": "^3.25.5",
    "dotenv": "^16.0.3",
    "vite": "^3.1.4",
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.8.2",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitest/coverage-c8": "^0.23.4",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.12.0",
    "cypress": "^10.9.0",
    "eslint": "^8.24.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.6.0",
    "jsdom": "^20.0.1",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.8.4",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.40.13"
  }
}
